																  <% 
																if(listgroup!=null)  listgroup.forEach((questiongroup) =>  {
																 { 
																if(group!=null)
																  if(questiongroup._id==group  ){%>

													<option value="<%=questiongroup._id%>" selected><%=questiongroup.name%></option>
															
												 <% }else
															{%>
													<option value="<%=questiongroup._id%>"><%=questiongroup.name%></option>
												 <%}
												 }}) %>



															
															</select>
															</h6>
							<div class="panel-wrapper collapse in">
								<div class="panel-body">
									<div class="table-wrap">
										<div class="table-responsive">
											<table id="example" class="table table-hover display  pb-30" >
												<thead>
													<tr id="tabletitle">
													<%  if(listkhaosat!=null)
													listkhaosat[0].data.forEach((itemkhaosat) => { %>
													<th><%=itemkhaosat.name%></th>

													<% })%>
														
													</tr>
												</thead>
												<tfoot>
													<tr id="tabletitle">
													<%  if(listkhaosat!=null)
													listkhaosat[0].data.forEach((itemkhaosat) => { %>
													<th><%=itemkhaosat.name%></th>

													<% })%>
														
													</tr>
												</tfoot>
												<tbody  id="tabletcontent">

												  <% 
												  if(listkhaosat!=null)
												  listkhaosat.forEach((khaosat) => { %>
												  <tr>
												  	<% 		  
												  khaosat.data.forEach((itemkhaosat) => { %>

														<td><span class="txt-dark weight-500"><%=itemkhaosat.value%></span></td>
												  <%})
													
												%></tr> <% })%>
													
												</tbody>
											</table>
										</div>
									</div>
								</div>
							</div>
						</div>	
					</div>
				</div>
